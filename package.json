{
  "name": "ngx-super-table",
  "description": "A table component for angular projects (angular versions >=2)",
  "homepage": "https://github.com/andyperlitch/ngx-super-table#readme",
  "bugs": {
    "url": "https://github.com/andyperlitch/ngx-super-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyperlitch/ngx-super-table.git"
  },
  "license": "MIT",
  "author": "Andy Perlitch",
  "version": "0.0.0",
  "angular-cli": {},
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dist": "./scripts/build.sh",
    "build:clean": "rimraf build dist waste deploy",
    "publish": "./scripts/publish.sh",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gh-pages": "git checkout gh-pages && git merge master && npm run build && npm run typedoc && git add . && git commit -m 'chore: build demo and docs' && git push && git checkout master",
    "prerelease": "npm test",
    "release": "standard-version --first-release && git push --follow-tags origin master && npm run build:dist && npm run publish",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "typedoc": "typedoc --options typedoc.json ngx-super-table.ts src/*.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "lodash": "^4.17.0",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.2",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.51",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "commitizen": "~2.8.1",
    "conventional-changelog": "~1.1.0",
    "conventional-changelog-cli": "~1.2.0",
    "cz-conventional-changelog": "~1.2.0",
    "ghooks": "~1.3.2",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "standard-version": "~2.4.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.1.0",
    "validate-commit-msg": "~2.8.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
